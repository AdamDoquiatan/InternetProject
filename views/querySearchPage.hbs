<div id="top-container">
  <div id="logo"><a href="#default"> <h1><span id="highlight">c</span>k</h1></a></div>
  <div id="search-bar">
  <form class="searchByQuery" action='/querySearchPage' method="GET">
  <input class="inp_searchBar" name="searchQuery" type="text" maxlength="40" placeholder="Search">
  <input class="btn_searchQuery" type="submit" value="search" />
  </form>
  </div>
  <form name="logout" method="post" action="">
  <label class="logoutLblPos">
  <input id="logout-btn" name="logoutbtn" type="submit" id="submit2" value="log out">
  </label>
</form>
</div>

<h1>Query Search Page</h1>
<h2>PostsData Placeholders Made </h2>

<p>{{post_data}}</p>

 <div class="container">
        {{#each people}}
        <div class="row">
            <div class="user-img">
                <img class="artistImg" style="width:50px;" src={{this.img_url}} />
            </div>
            <div class="results-container">
                <div class="row">
                    <div class="title">
                        <h6>{{this.title}}</h6>
                    </div>
                    <div class="col-md-4">
                        <button type="button" class="btn-info">{{this.topic}}</button>
                    </div>
                </div>
                <div class="row">
                    <div class="topic">
                        <p>{{this.topic_desc}}</p>
                    </div>
                </div>
                <div class="row">
                    <div class="date">
                        <p>{{this.date}}</p>
                    </div>
      
                </div>
            </div>
        </div>
        {{/each}}
    </div>
<p>{{ post_data }}</p>